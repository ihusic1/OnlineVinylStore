<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #fefff9;" xmlns="http://javafx.com/javafx/17.0.8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label layoutX="144.0" layoutY="50.0" text="My Orders">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <font>
                <Font name="Century Gothic" size="30.0" />
            </font>
        </Label>
        <Button fx:id="close" cancelButton="true" layoutX="35.0" layoutY="36.0" mnemonicParsing="false" onAction="#actionClose" style="-fx-background-color: #fefff9;" text="&lt;&lt;">
            <font>
                <Font name="Century Gothic Bold" size="14.0" />
            </font></Button>
        <TableView fx:id="listTable" layoutX="110.0" layoutY="113.0" prefHeight="270.0" prefWidth="237.0" style="-fx-background-color: #fefff9; -fx-border-color: grey;">
            <columns>
                <TableColumn fx:id="titleColumn" prefWidth="78.4000244140625" text="Title" />
                <TableColumn fx:id="artistColumn" prefWidth="85.5999755859375" text="Artist" />
                <TableColumn fx:id="priceColumn" prefWidth="72.79998779296875" text="Price" />
            </columns>
        </TableView>
        <ImageView fitHeight="248.0" fitWidth="246.0" layoutX="366.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/myorderspic.png" />
            </image>
        </ImageView>
    </children>
</AnchorPane>
